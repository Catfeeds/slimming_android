<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".fragment.diet.DietFragment">



    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:padding="@dimen/padding"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="@string/query_calorie"
                    android:textColor="@color/black"
                    android:textSize="25sp" />

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin"
                    android:shrinkColumns="3">

                    <TableRow>

                        <LinearLayout
                            android:id="@+id/category1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:clickable="true"
                            android:focusable="true">

                            <de.hdodenhof.circleimageview.CircleImageView
                                android:layout_width="@dimen/categoryImgWidth"
                                android:layout_height="@dimen/categoryImgHeight"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/category1" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:text="@string/category1" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/category2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <de.hdodenhof.circleimageview.CircleImageView
                                android:layout_width="@dimen/categoryImgWidth"
                                android:layout_height="@dimen/categoryImgHeight"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/category2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:text="@string/category2" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/category3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <de.hdodenhof.circleimageview.CircleImageView
                                android:layout_width="@dimen/categoryImgWidth"
                                android:layout_height="@dimen/categoryImgHeight"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/category3" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:text="@string/category3" />
                        </LinearLayout>
                    </TableRow>

                    <TableRow android:layout_marginTop="@dimen/margin">

                        <LinearLayout
                            android:id="@+id/category4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <de.hdodenhof.circleimageview.CircleImageView
                                android:layout_width="@dimen/categoryImgWidth"
                                android:layout_height="@dimen/categoryImgHeight"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/category4" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:text="@string/category4" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/category5"
                            android:layout_width="wrap_content"
